= form_for @dive_center do |f|
  -if @dive_center.errors.any?
    #error_explanation
      %h2= "#{pluralize(@dive_center.errors.count, "error")} prohibited your information from being saved:"
      %ul
        - @dive_center.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :url
    = f.text_field :url
  .field
    = f.label :email
    = f.text_field :email
  .actions
    = f.submit 'Register!'
